{
    "name": "conorhaining/phpopenraildata",
    "description": "A package which uses the Network Rail & National Rail feeds to provide timetable, service, performance and tracking data about the UK rail network.",
    "type": "library",
    "license": "MIT",
    "version": "0.0.1-dev",
    "authors": [
        {
            "name": "Conor Haining",
            "email": "conor.haining@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "stomp-php/stomp-php": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "autoload": {
        "psr-4": {
            "OpenRail\\": "src/"
        }
    }
}
